    "express": "^4.17.3",
    "helmet": "^8.0.0",
    "ion-cortex": "^1.3.6",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "mongoose": "^8.9.2",
    "nanoid": "^3.3.2",
    "oyster-db": "*",
    "qantra-pineapple": "^1.0.7",
    "request-ip": "^2.1.3",
